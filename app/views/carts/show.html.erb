<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
  <div class="container-fluid">
    <a class="navbar-brand" href="#">myStore</a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav me-auto mb-2 mb-lg-0">

        <li class="r4"><%= link_to'Home',root_path,class: "nav-link" %></li>
        <li><%= link_to'Products',products_path, class:"nav-link"%></li>
        <li ><%= link_to'Orders',orders_path, class: "nav-link"%></li>
        <li ><%= link_to'MyCart',cart_path(current_user), class: "nav-link active"%></li>
        <li ><%= link_to'Profile',user_path(current_user), class: "nav-link"%></li>
      </ul>
      
    </div>
  </div>
</nav>


<div style="margin-top:30px">
  <% if alert %>
    <p class="alert alert-danger"><%= alert %></p>
  <% end %>
<%if current_user.cart.present? %>
  <% if current_user.cart.products.present?%>
    <% @cart.products.each do |product| %>
      <section style="background-color: #eee;">
        <div class="container py-1">
          <div class="row justify-content-center mb-3">
            <div class="col-md-12 col-xl-10">
              <div class="card shadow-0 border rounded-3">
                <div class="card-body">
                  <div class="row">
                    <div class="col-md-12 col-lg-3 col-xl-3 mb-4 mb-lg-0">
                      <div class="bg-image hover-zoom ripple rounded ripple-surface">
                        <% if product.image.attached? %>
                            <%= image_tag(product.image, class: "image")%>
                        <% end%>
                        <a href="#!">
                          <div class="hover-overlay">
                            <div class="mask" style="background-color: rgba(253, 253, 253, 0.15);"></div>
                          </div>
                        </a>
                      </div>
                    </div>
                    <div class="col-md-6 col-lg-6 col-xl-6">
                      <h5><strong><%= product.name %></strong></h5>
                      <div class="d-flex flex-row">
                        <div class="text mb-1 me-2">
                          <div class="fa fa-star"></div>
                          <div class="fa fa-star"></div>
                          <div class="fa fa-star"></div>
                          <div class="fa fa-star"></div>
                        </div>
                      </div>
                      <p class="text-truncate mb-4 mb-md-0">
                          <%= product.desc %>
                      </p>
                      <p>Seller:
                          <%= product.seller.name %>
                      </p>
                      
                      <div class="d-flex flex-row align-items-center mb-1">
                          <h4 class="mb-1 me-1 text-success">₹<%= product.price %></h4>
                      </div>
                        <%= form_with scope: :product, url: products_path do |form| %>
                        <%= hidden_field_tag :product_id, product.id %>
                        <%= form.number_field :quantity, in: 1..20, value: product.req_quantity %>
                        <%= form.submit :Update,class:"btn btn-secondary btn-sm" %>
                        <%end%>
                      <div class="d-flex flex-row gap-2 col-6">
                            <%= link_to 'Show', product_path(product), class:"btn btn-primary mt-2" %>
                            <%= button_to  'remove', cart_path(:product_id => product.id ,:id => @cart.id),
                                    method: :delete, data: { confirm: 'Are you sure?' } ,class:"btn btn-danger  mt-2"%>   
                        </div>
                    </div>
                    
                  </div>
                </div>
                
              </div>
              
            </div>
          </div>   
      </section>
    <%end%>
  <div class="d-grid gap-2 col-6 mx-auto">
   <h5 style="font-family:serif"><strong>Cart total: ₹<%= @cart.total%></strong></h5>
  </div>
  
  <div class="d-grid gap-2 col-6 mx-auto">
   <%= link_to 'Proceed', payments_path, class:"btn btn-success btn-sm mb-4 py-8  " %>
  </div>
  
  <%else%>
    <div class="container-fluid  mt-100">
            <div class="row">
            
              <div class="col-md-12">
              
                  <div class="card">
                <div class="card-header">
                </div>
                <div class="card-body cart">
                    <div class="col-sm-12 empty-cart-cls text-center">
                      <img src="data:image/png;base64," width="130" height="130" class="img-fluid mb-4 mr-3">
                      <h3><strong>Your Cart is Empty</strong></h3>
                      <h4>add something</h4>
                      <%= link_to 'Continue Sopping',root_path, class:"btn btn-primary cart-btn-transform m-3"%>							
                    </div>
                </div>
            </div>
    </div>
  
  <%end%>
<%else%>
  <div class="container-fluid  mt-100">
            <div class="row">
            
              <div class="col-md-12">
              
                  <div class="card">
                <div class="card-header">
                </div>
                <div class="card-body cart">
                    <div class="col-sm-12 empty-cart-cls text-center">
                      <img src="data:image/png;base64," width="130" height="130" class="img-fluid mb-4 mr-3">
                      <h3><strong>Your Cart is Empty</strong></h3>
                      <h4>add something</h4>
                      <%= link_to 'Continue Sopping',root_path, class:"btn btn-primary cart-btn-transform m-3"%>							
                    </div>
                </div>
            </div>
    </div>
<%end%>
</div>


