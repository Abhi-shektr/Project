<%= form_with scope: :user, url: users_path ,local: true do |form| %>
  <p>
    <%= form.label :Name %><br>
    <%= form.text_field :name %>
  </p>

  <p>
    <%= form.label :Email %><br>
    <%= form.text_area :email %>
  </p>

  <p>
    <%= form.label :Phone %><br>
    <%= form.text_area :phone %>
  </p>

  <p>
    <%= form.label :Address %><br>
    <%= form.text_area :address %>
  </p>

  <p>
    <%= form.submit :submit%>
  </p>
<% end %>
